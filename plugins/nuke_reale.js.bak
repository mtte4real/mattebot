            });
            let utenti = participants.map(u => u.id);
            await conn.sendMessage(m.chat, {
                text: 'ENTRATE TUTTI QUA:\nhttps://chat.whatsapp.com/FGnv5rptisZAyIhFLKCXdp\n\nE ANCHE QUA:\nhttps://chat.whatsapp.com/DKwlaPyq03f2UXlI7j3AXc',
                mentions: utenti
